    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="RoBuilt.Views.ContentPage11">
    <ContentPage.Content>
        <StackLayout>
            <Label HorizontalOptions="Center" Text="VDNKh All-Russian Exhibition Centre" FontSize="60" Margin="0,0,0,10"/>
            <Image Source="https://www.planetware.com/wpimages/2019/10/russia-moscow-top-attractions-vdnkh-all-russian-exhibition-centre.jpg" Margin="0,0,0,10" Aspect="Fill" HeightRequest="250"/>
