<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="RoBuilt.Views.ContentPage6">
    <ContentPage.Content>
        <StackLayout>
            <Label HorizontalOptions="Center" Text="Museum of Cosmonautics" FontSize="60" Margin="0,0,0,10"/>
            <Image Source="https://www.planetware.com/wpimages/2019/10/russia-moscow-top-attractions-museum-of-cosmonautics.jpg" Margin="0,0,0,10" Aspect="Fill" HeightRequest="250"/>
            <Label FontSize="20" Text="At one point, Russia and the US were toe-to-toe when it came to space exploration. While that might no longer be the case, the museum's amazing collection-which includes over 85,000 items-is still awe-inspiring."/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
