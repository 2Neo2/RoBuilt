    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="RoBuilt.Views.AccountPage"
    xmlns:local="clr-namespace:RoBuilt.Views"
    Title = "OwnAccount">
    <FlyoutItem Title="Menu">
        <ShellContent Title="Frineds" ContentTemplate="{DataTemplate local:FriendsPage}"/>
        <ShellContent Title="Meetiings" ContentTemplate="{DataTemplate local:MeetingPage}"/>
        <ShellContent Title="Progress" ContentTemplate="{DataTemplate local:ProgressPage}"/>
